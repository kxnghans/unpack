else
  export NODE_PATH="/mnt/c/Users/kobby/Downloads/gitProjects/unpack/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.3_@babel+preset-env@7.28.3_@babel+core@7.28.3__react-native@0.7_3qrtmotrwi3pp4riqzpzxvq2cy/node_modules/expo/bin/node_modules:/mnt/c/Users/kobby/Downloads/gitProjects/unpack/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.3_@babel+preset-env@7.28.3_@babel+core@7.28.3__react-native@0.7_3qrtmotrwi3pp4riqzpzxvq2cy/node_modules/expo/node_modules:/mnt/c/Users/kobby/Downloads/gitProjects/unpack/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.3_@babel+preset-env@7.28.3_@babel+core@7.28.3__react-native@0.7_3qrtmotrwi3pp4riqzpzxvq2cy/node_modules:/mnt/c/Users/kobby/Downloads/gitProjects/unpack/node_modules/.pnpm/node_modules:$NODE_PATH"
